{
    "name": "@ermish/shuji",
    "version": "1.0.0",
    "author": "ermish <philipermish@gmail.com>",
    "license": "MIT",
    "description": "A Markdown to React JSX converter",
    "keywords": [
        "react",
        "markdown",
        "template",
        "remark",
        "jsx",
        "front matter",
        "converter"
      ],
    "repository": "ermish/shuji",
    "main": "dist/index.js",
    "scripts": {
        "build": "yarn && parcel build ./src/index.ts --dist-dir dist",
        "start": "yarn && parcel watch ./src/index.ts --dist-dir dist",
        "lint": "eslint ./src/**.js",
        "test": "jest --verbose"
    },
    "dependencies": {
        "fs-extra": "10.0.0",
        "json5": "^2.2.0",
        "react-markdown": "^6.0.2",
        "remark-gfm": "^1.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.0",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.13.13",
        "@babel/preset-typescript": "^7.13.0",
        "@tsconfig/node16": "^1.0.0",
        "@types/fs-extra": "^9.0.11",
        "@types/jest": "^26.0.20",
        "@types/json5": "^2.2.0",
        "@types/klaw": "^3.0.1",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "babel-jest": "^26.6.3",
        "eslint": "7.17.0",
        "eslint-config-prettier": "^7.1.0",
        "gray-matter": "^4.0.2",
        "jest": "^26.6.3",
        "parcel@next": "2.0.0-beta.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "sass": "1.32.0",
        "stylelint": "13.8.0",
        "stylelint-config-recommended": "3.0.0",
        "stylelint-order": "4.1.0",
        "stylelint-scss": "3.18.0",
        "typescript": "4.2.4"
