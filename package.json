        "yargs": "^17.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.14.0",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.13.13",
        "@babel/preset-typescript": "^7.13.0",
        "@tsconfig/node16": "^1.0.1",
        "@types/fs-extra": "^9.0.11",
