        "@types/json5": "^2.2.0",
        "@types/klaw": "^3.0.1",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "babel-jest": "^26.6.3",
        "eslint": "^7.27.0",
        "eslint-config-prettier": "^7.1.0",
        "jest": "^26.6.3",
        "parcel": "^2.0.0-beta.3.1",
        "sass": "^1.32.0",
        "stylelint": "^13.8.0",
        "stylelint-config-recommended": "^3.0.0",
